{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "dotenv -- turbo dev",
    "build": "dotenv -- turbo build",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {}
}
